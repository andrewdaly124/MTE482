{
  "name": "andrews-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "airbnb": "0.0.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "eslint-config-airbnb": "^19.0.4",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.0",
    "typesafe-actions": "^5.1.0",
    "wait-on": "^6.0.0",
    "web-vitals": "^2.1.3"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "lint": "eslint --fix --ext .js,.jsx *.js*",
    "elecserve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run elecstart\"",
    "elecbuild": "npm run build && electron-builder -c.extraMetadata.main=build/main.js",
    "elecstart": "wait-on tcp:3000 && electron ."
  },
  "build": {
    "extends": null,
    "appId": "com.example.electron-cra",
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^16.0.6",
    "electron-builder": "^22.14.5",
    "eslint": "^8.7.0"
  }
}
